{
  "name": "dinerparty",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve src/client/main.js",
    "build": "vue-cli-service build src/client/main.js",
    "develop": "nodemon --watch src/client",
    "serve_build": "yarn watch ",
    "lint": "vue-cli-service lint",
    "server": "export FLASK_ENV=development && export FLASK_APP=src/server/app.py && pipenv run flask run",
    "both": "export FLASK_ENV=development ; export FLASK_APP=src/server/app.py ; pipenv run flask run & vue-cli-service serve src/client/main.js && fg"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/vue-fontawesome": "^0.1.4",
    "axios": "^0.18.1",
    "glyphicons": "^0.2.0",
    "jquery": "^3.5",
    "moments": "^0.0.2",
    "object-to-formdata": "^1.6.4",
    "promise": "^8.0.2",
    "vee-validate": "^2.2.2",
    "vue": "^2.5.17",
    "vue-material": "^1.0.0-beta-11",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "node-sass": "^7.0.0",
    "npm-watch": "^0.5.0",
    "sass-loader": "^7.1.0",
    "scss-loader": "^0.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
